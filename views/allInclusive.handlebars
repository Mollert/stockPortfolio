
	<div class="mainTub">

		<div class="siteTitle">A listing of all transactions</div>

		<div class="flexRow listRails">
			{{#each allTrans}}
			<div>
				<div >A {{this.ticker}} <span style="color: {{this.typeColor}};">{{this.type}}</span> on {{this.date}},</div>
				<div class="indentList">{{this.shares}} shares @ {{this.perShare}} per share; equaling {{this.total}} total cost.</div>
				<div style="height: {{divHeight}}rem"></div>
			</div>
			{{/each}}
		</div>

		<div class="goingHome flexRow">This will take you back to the&nbsp;<a href="/">home page</a>.</div>	

	</div>